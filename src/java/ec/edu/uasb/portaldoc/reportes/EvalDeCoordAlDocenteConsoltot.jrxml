	<subDataset name="PromProgPorPregunta" uuid="539cff77-839a-4919-b59b-f501a7acd4e0">
		<parameter name="codEspe" class="java.lang.String"/>
		<parameter name="anioConsul" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select TABDOC.CODIGO_PREGUNTA, TABDOC.PREGUNTA, CONVERT(decimal(8, 2), AVG(TABDOC.VALOR)) AS VALOR
FROM            VISTA_TABULACION_EVALDOCENTEANIO AS TABDOC INNER JOIN
                         ENCUESTA_CALENDARIO ON TABDOC.ANIO = ENCUESTA_CALENDARIO.ANIO AND TABDOC.CICLO = ENCUESTA_CALENDARIO.CICLO AND
                         TABDOC.CODIGO_ENCUESTA = ENCUESTA_CALENDARIO.CODIGO_ENCUESTA AND TABDOC.CODIGO_MATERIA = ENCUESTA_CALENDARIO.CODIGO_MATERIA AND
                         TABDOC.CODIGO_NIVEL = ENCUESTA_CALENDARIO.CODIGO_NIVEL AND TABDOC.CODIGO_PROFESOR = ENCUESTA_CALENDARIO.CODIGO_PROFESOR AND
                         TABDOC.CODIGO_ESP = ENCUESTA_CALENDARIO.CODIGO_ESP INNER JOIN
                         PENSUM_MATERIA ON TABDOC.ANIO = PENSUM_MATERIA.ANIO AND TABDOC.CICLO = PENSUM_MATERIA.CICLO AND
                         TABDOC.CODIGO_MATERIA = PENSUM_MATERIA.CODIGO_MATERIA AND TABDOC.CODIGO_ESP = PENSUM_MATERIA.CODIGO_ESP INNER JOIN
                         MATERIA_DICTAR ON PENSUM_MATERIA.ANIO = MATERIA_DICTAR.ANIO AND PENSUM_MATERIA.CICLO = MATERIA_DICTAR.CICLO AND
                         PENSUM_MATERIA.CODIGO_MATERIA = MATERIA_DICTAR.CODIGO_MATERIA AND PENSUM_MATERIA.CODIGO_NIVEL = MATERIA_DICTAR.CODIGO_NIVEL AND
                         TABDOC.CODIGO_PROFESOR = MATERIA_DICTAR.CODIGO_PROFESOR
WHERE  ENCUESTA_CALENDARIO.TIPO_ENCUESTA = 'C' AND TABDOC.ANIO = $P{anioConsul}
AND PENSUM_MATERIA.CODIGO_ESP=$P{codEspe}
GROUP by TABDOC.CODIGO_PREGUNTA,TABDOC.PREGUNTA
ORDER BY 1]]>
		</queryString>
		<field name="CODIGO_PREGUNTA" class="java.math.BigDecimal"/>
		<field name="PREGUNTA" class="java.lang.String"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="PromProgHistoricoXPregunta" uuid="0ed444ff-e300-433e-b678-70cf289c9dc3">
		<parameter name="codEspe" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT TABDOC.ANIO, convert(decimal(8,2),avg(TABDOC.VALOR)) as VALOR
FROM            VISTA_TABULACION_EVALDOCENTEANIO AS TABDOC INNER JOIN
                         ENCUESTA_CALENDARIO ON TABDOC.ANIO = ENCUESTA_CALENDARIO.ANIO AND TABDOC.CICLO = ENCUESTA_CALENDARIO.CICLO AND
                         TABDOC.CODIGO_ENCUESTA = ENCUESTA_CALENDARIO.CODIGO_ENCUESTA AND TABDOC.CODIGO_MATERIA = ENCUESTA_CALENDARIO.CODIGO_MATERIA AND
                         TABDOC.CODIGO_NIVEL = ENCUESTA_CALENDARIO.CODIGO_NIVEL AND TABDOC.CODIGO_PROFESOR = ENCUESTA_CALENDARIO.CODIGO_PROFESOR AND
                         TABDOC.CODIGO_ESP = ENCUESTA_CALENDARIO.CODIGO_ESP INNER JOIN
                         PENSUM_MATERIA ON TABDOC.ANIO = PENSUM_MATERIA.ANIO AND TABDOC.CICLO = PENSUM_MATERIA.CICLO AND
                         TABDOC.CODIGO_MATERIA = PENSUM_MATERIA.CODIGO_MATERIA AND TABDOC.CODIGO_ESP = PENSUM_MATERIA.CODIGO_ESP INNER JOIN
                         MATERIA_DICTAR ON PENSUM_MATERIA.ANIO = MATERIA_DICTAR.ANIO AND PENSUM_MATERIA.CICLO = MATERIA_DICTAR.CICLO AND
                         PENSUM_MATERIA.CODIGO_MATERIA = MATERIA_DICTAR.CODIGO_MATERIA AND PENSUM_MATERIA.CODIGO_NIVEL = MATERIA_DICTAR.CODIGO_NIVEL AND
                         TABDOC.CODIGO_PROFESOR = MATERIA_DICTAR.CODIGO_PROFESOR
WHERE  ENCUESTA_CALENDARIO.TIPO_ENCUESTA = 'C'
AND PENSUM_MATERIA.CODIGO_ESP in (select CODIGO_ESP from especializacion
where CODIGO_ESP_COM = (SELECT DISTINCT CODIGO_ESP_COM FROM especializacion WHERE CODIGO_ESP = $P{codEspe})
and duracion_esp = 1
and anio  = TABDOC.anio)
GROUP by TABDOC.ANIO
ORDER BY 1]]>
		</queryString>
		<field name="ANIO" class="java.math.BigDecimal"/>
		<field name="VALOR" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="anio" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT        CICLO_ACADEMICO.NOMBRE_CICLO, TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_ESP,
                         TABDOC.CODIGO_MATERIA, TABDOC.CODIGO_ENCUESTA, PROFESOR.NOMBRE_PROFESOR,
                         PROFESOR.APELLIDO_PROFESOR, PROFESOR.CODIGO_PROFESOR, MATERIA.NOMBRE_MATERIA,TABDOC.CODIGO_NIVEL, NIVEL.NOMBRE_NIVEL, FACULTAD.NOMBRE_FACULTAD,
                         NIVEL_ACADEMICO.NOMBRE_NIVEACAD, ESCUELA.NOMBRE_ESCUELA, ESPECIALIZACION.NOMBRE_ESP, MATERIA_DICTAR.HORAS_MATERIA_DICTAR,
                         CONVERT(decimal(8, 2), AVG(TABDOC.VALOR)) AS VALOR,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,1,TABDOC.CODIGO_NIVEL) AS UNO,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,2,TABDOC.CODIGO_NIVEL) AS DOS,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,3,TABDOC.CODIGO_NIVEL) AS TRES,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,4,TABDOC.CODIGO_NIVEL) AS CUATRO,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,5,TABDOC.CODIGO_NIVEL) AS CINCO,
dbo.f_DOCENTE_PROM_PREGUNTA(TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA,6,TABDOC.CODIGO_NIVEL) AS SEIS
FROM 	CICLO_ACADEMICO INNER JOIN
	PROFESOR INNER JOIN
          VISTA_TABULACION_EVALDOCENTEANIO as TABDOC
	ON PROFESOR.CODIGO_PROFESOR = TABDOC.CODIGO_PROFESOR
	ON CICLO_ACADEMICO.ANIO = TABDOC.ANIO AND
          CICLO_ACADEMICO.CICLO = TABDOC.CICLO INNER JOIN MATERIA
	INNER JOIN NIVEL
	INNER JOIN PENSUM_MATERIA
	ON NIVEL.CODIGO_NIVEL = PENSUM_MATERIA.CODIGO_NIVEL
	ON MATERIA.ANIO = PENSUM_MATERIA.ANIO AND
          MATERIA.CICLO = PENSUM_MATERIA.CICLO AND MATERIA.CODIGO_MATERIA = PENSUM_MATERIA.CODIGO_MATERIA INNER JOIN FACULTAD
          ON PENSUM_MATERIA.CODIGO_FACULTAD = FACULTAD.CODIGO_FACULTAD INNER JOIN NIVEL_ACADEMICO
          ON PENSUM_MATERIA.CODIGO_NIVEACAD = NIVEL_ACADEMICO.CODIGO_NIVEACAD INNER JOIN  ESCUELA
	ON PENSUM_MATERIA.CODIGO_FACULTAD = ESCUELA.CODIGO_FACULTAD AND
	PENSUM_MATERIA.CODIGO_ESCUELA = ESCUELA.CODIGO_ESCUELA INNER JOIN ESPECIALIZACION
	ON PENSUM_MATERIA.ANIO = ESPECIALIZACION.ANIO AND PENSUM_MATERIA.CICLO = ESPECIALIZACION.CICLO AND
	PENSUM_MATERIA.CODIGO_NIVEACAD = ESPECIALIZACION.CODIGO_NIVEACAD AND
	PENSUM_MATERIA.CODIGO_FACULTAD = ESPECIALIZACION.CODIGO_FACULTAD AND
	PENSUM_MATERIA.CODIGO_ESCUELA = ESPECIALIZACION.CODIGO_ESCUELA
AND PENSUM_MATERIA.CODIGO_ESP = ESPECIALIZACION.CODIGO_ESP INNER JOIN  MATERIA_DICTAR
	ON PENSUM_MATERIA.ANIO = MATERIA_DICTAR.ANIO AND PENSUM_MATERIA.CICLO = MATERIA_DICTAR.CICLO AND
	PENSUM_MATERIA.CODIGO_MATERIA = MATERIA_DICTAR.CODIGO_MATERIA AND PENSUM_MATERIA.CODIGO_NIVEL = MATERIA_DICTAR.CODIGO_NIVEL
	ON TABDOC.ANIO = PENSUM_MATERIA.ANIO AND TABDOC.CICLO = PENSUM_MATERIA.CICLO AND
	TABDOC.CODIGO_MATERIA = PENSUM_MATERIA.CODIGO_MATERIA AND
	TABDOC.CODIGO_ESP = PENSUM_MATERIA.CODIGO_ESP AND
	TABDOC.CODIGO_NIVEL = PENSUM_MATERIA.CODIGO_NIVEL AND
	TABDOC.CODIGO_PROFESOR = MATERIA_DICTAR.CODIGO_PROFESOR
WHERE  TABDOC.TIPO_ENCUESTA = 'C' AND TABDOC.ANIO = $P{anio}
GROUP BY CICLO_ACADEMICO.NOMBRE_CICLO, TABDOC.ANIO, TABDOC.CICLO,TABDOC.CODIGO_PROFESOR,TABDOC.CODIGO_MATERIA,TABDOC.CODIGO_ESP, TABDOC.CODIGO_ENCUESTA, PROFESOR.NOMBRE_PROFESOR,
                         PROFESOR.APELLIDO_PROFESOR, PROFESOR.CODIGO_PROFESOR, MATERIA.NOMBRE_MATERIA, NIVEL.NOMBRE_NIVEL,
                         TABDOC.CODIGO_NIVEL, FACULTAD.NOMBRE_FACULTAD, NIVEL_ACADEMICO.NOMBRE_NIVEACAD,
                         ESCUELA.NOMBRE_ESCUELA, ESPECIALIZACION.NOMBRE_ESP, MATERIA_DICTAR.HORAS_MATERIA_DICTAR
ORDER BY TABDOC.ANIO,FACULTAD.NOMBRE_FACULTAD,TABDOC.CODIGO_ESP, TABDOC.CODIGO_NIVEL, PROFESOR.APELLIDO_PROFESOR,PROFESOR.NOMBRE_PROFESOR, MATERIA.NOMBRE_MATERIA]]>
	</queryString>
